

<div class="container" style="margin-top: 30px;">
    <h1>Liste user</h1>
    <table class="table">
        <tr>
            <th>Id</th>
            <th>Email</th>
        </tr>

        <% _.each(users, function(user) { %>
            <tr data-id="<%= user.id %>" data-model="user">
                <td><%= user.id %></td>
                <td><%= user.email %></td>
            </tr>
        <% }) %>

    </table>

</div>


